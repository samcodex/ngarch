{
  "name": "ngArch",
  "version": "2.3.0",
  "description": "Angular application architecture tool, analyzing Angular application and visually displaying the modules, components, services and data models in multiple diagrams",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build --configuration production",
    "copy_readme": "cpx README.md dist",
    "copy_dist_package": "cpx package-dist.json dist",
    "rename_dist_package": "node -e \"require('fs').rename('dist/package-dist.json', 'dist/package.json', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\"",
    "copy_files": "npm run copy_readme && npm run copy_dist_package && npm run rename_dist_package",
    "pack": "cd ./dist && npm pack",
    "package": "npm run build && npm run copy_files && npm run pack",
    "publish": "cd ./dist && npm publish",
    "ngarch-server": "node node_modules/ngarch-server/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.2",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^12.1.2",
    "@angular/compiler": "^12.1.2",
    "@angular/core": "^12.1.2",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "^12.1.2",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "^12.1.2",
    "@angular/platform-browser-dynamic": "^12.1.2",
    "@angular/router": "^12.1.2",
    "@types/d3": "^4.13.2",
    "ajv": "^6.12.2",
    "angular-resizable-element": "^3.4.0",
    "angular-tree-component": "^8.5.6",
    "angular2-draggable": "^2.3.2",
    "core-js": "^2.6.11",
    "d3": "^4.13.0",
    "express": "^4.17.1",
    "flatted": "^2.0.2",
    "lodash-es": "^4.17.21",
    "material-design-icons": "^3.0.1",
    "minimist": "^1.2.5",
    "ng-arch-ui": "^1.1.0",
    "ng2-movable": "^0.3.1",
    "ngx-swiper-wrapper": "^9.0.1",
    "ngx-toastr": "^12.1.0",
    "popper.js": "^1.16.1",
    "prismjs": "^1.24.1",
    "rxjs": "^6.6.7",
    "socket.io": "^2.3.0",
    "socks": "^2.6.1",
    "take-until-ng-destroy": "^0.0.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.1.2",
    "@angular/cli": "^12.1.2",
    "@angular/compiler-cli": "^12.1.2",
    "@angular/language-service": "^12.1.2",
    "@types/jasmine": "^3.6.11",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^12.20.15",
    "codelyzer": "^6.0.2",
    "cpx": "^1.5.0",
    "express-flash": "0.0.2",
    "express-validator": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "lusca": "^1.7.0",
    "morgan": "^1.10.0",
    "ngarch-server": "^1.4.5",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "protractor": "~7.0.0",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "typescript": "4.3.5"
  }
}
